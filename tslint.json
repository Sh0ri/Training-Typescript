{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"quotemark": [true, "double", "avoid-template"],
		"semicolon": [true, "never"],
		"indent": [false, "tabs"],
		"no-var-requires": false,
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
		"no-console": [true, "assert", "clear", "count", "debug", "dir", "dirxml", "error", "group", "groupCollapsed", "groupEnd", "info", "table", "time", "timeEnd", "trace", "warn"],
		"no-any": false,
		"ordered-imports": false,
		"max-classes-per-file": false,
		"max-line-length": [false, 150],
		"object-literal-sort-keys": false,
		"no-return-await": true,
		"ignoreDefinitionFiles": false,
		"only-arrow-functions": true,
		"jsx-no-lambda": true,
		"jsx-no-bind": true,
		"jsx-no-multiline-js": false,
		"jsx-alignment": false,
		"no-floating-promises": [true, "Bluebird"],
		"await-promise": [true, "Bluebird"]
	},
	"rulesDirectory": []
}